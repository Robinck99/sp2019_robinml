    "import cv2 as cv\n",
    "import os \n",
    "\n",
    "path = os.getcwd() + \"/video/intro.mp4\"\n",
    "\n",
    "vid = cv.VideoCapture(path)\n",
    "\n",
    "getable = 1\n",
    "count = 0\n",
    "\n",
    "while getable:\n",
    "    getable, frm = vid.read()\n",
    "    cv.imwrite(os.getcwd() + \"/video/frame%d.jpg\" % count, frm)\n",
    "    count += 1"
   ]
